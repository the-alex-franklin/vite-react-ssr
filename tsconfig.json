{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "outDir": "dist",
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
  }
}
